{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "noImplicitThis": false,
    /* Alias */
    "baseUrl": "./",
    "paths": {
      "@assets": [
        "./src/assets"
      ],
      "@component/*": [
        "./src/components/*"
      ],
      "@config/*": [
        "./src/configs/*"
      ],
      "@layout/*": [
        "./src/layouts/*"
      ],
      "@schema/*": [
        "./src/schema/*"
      ],
      "@core/*": [
        "./src/core/*"
      ],
    }
  }
}